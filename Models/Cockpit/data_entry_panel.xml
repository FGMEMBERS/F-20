<!-- HUD ******************************************************************* -->
  <animation>
  <type>rotate</type>
    <object-name>HUDknob</object-name>
	<property>f-20/HUD/intensity</property>
	<factor>-20.0</factor>
	<offset-deg>-20</offset-deg>
  <center>
   <x-m>-3.91067</x-m>
   <y-m>-0.0326</y-m>
   <z-m>0.35599</z-m>
  </center>
  <axis>
   <x>0.991267569</x>
   <y>0</y>
   <z>-0.131865869</z>
  </axis>
 </animation>
 
  <animation>
  <type>pick</type>
   <object-name>HUDknob</object-name>
   <action>
    <button>3</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
		<command>nasal</command>
		<script>f20.increaseIntensity();</script>
    </binding>
   </action>
 </animation>

 <animation>
  <type>pick</type>
   <object-name>HUDknob</object-name>
   <action>
    <button>4</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
		<command>nasal</command>
		<script>f20.decreaseIntensity();</script>
    </binding>
   </action>
 </animation>
 
  <animation>
  <type>rotate</type>
    <object-name>stbyReticle</object-name>
	<property>/f-20/HUD/stand-by-switch</property>
	<factor>-60.0</factor>
	<offset-deg>30</offset-deg>
  <center>
   <x-m>-3.92093</x-m>
   <y-m>0.03018</y-m>
   <z-m>0.35903</z-m>
  </center>
  <axis>
   <x>0</x>
   <y>1</y>
   <z>0</z>
  </axis>
 </animation>
 
  <animation>
  <type>pick</type>
   <object-name>stbyReticle</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
		<command>nasal</command>
		<script>f20.HUD_page.toggleStandbyReticle();</script>
    </binding>
   </action>
 </animation>
 
 
