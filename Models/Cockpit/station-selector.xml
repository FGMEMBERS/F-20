<PropertyList>

<!-- Stores management panel *******************************************************-->

<!-- buttons ****************************************************** -->

<!-- Tip left -->

 <animation>
  <type>translate</type>
	<object-name>TipL</object-name>
    <property>/f-20/stores/management-panel/TipL/pressed</property>
    <factor>-0.001</factor>
  <axis>
   <x>1.0</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

  <animation>
  <type>pick</type>
   <object-name>TipL</object-name>
   <action>
    <button>0</button>
    <repeatable>false</repeatable>
    <interval-sec>0.2</interval-sec>
    <binding>
		<command>nasal</command>
		<script>f20.pressStationButton (0);</script>
    </binding>
   </action>
 </animation>

<animation>
	
  <type>texmultiple</type>
  <object-name>TipL</object-name>

  <transform>
	<subtype>textranslate</subtype>
	<property>/f-20/stores/management-panel/TipL/x-shift</property>
	<factor>0.5</factor>
	<axis>
		<x>1</x>
		<y>0</y>
	</axis>
  </transform>

  <transform>
	<subtype>textranslate</subtype>
	<property>/f-20/stores/management-panel/TipL/y-shift</property>
	<factor>0.5</factor>
	<axis>
		<x>0</x>
		<y>-1</y>
	</axis>
  </transform>

</animation>

