<!-- throttle ****************************************************-->
 <animation>
  <type>rotate</type>
  <object-name>Throttle</object-name>
  <object-name>ThrottleStrut</object-name>
  <object-name>CounterMeasures</object-name>
  <object-name>CounterMeasures</object-name>
  <object-name>TDC</object-name>
  <object-name>Mike</object-name>
  <object-name>Flaps</object-name>
  <object-name>Undesignate</object-name>
  <object-name>CounterMeasures</object-name>
  <object-name>Cage</object-name>
  <object-name>SpeedBrakes</object-name>
  <property>/controls/engines/engine/display-throttle</property>
  <factor>-30</factor>
  <offset-deg>15</offset-deg>
  <center>
   <x-m>-3.70817</x-m>
   <y-m>0</y-m>
   <z-m>-0.27133</z-m>
  </center>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

  <animation>
  <type>translate</type>
  <object-name>Throttle</object-name>
  <object-name>ThrottleStrut</object-name>
  <object-name>CounterMeasures</object-name>
  <object-name>CounterMeasures</object-name>
  <object-name>TDC</object-name>
  <object-name>Mike</object-name>
  <object-name>Flaps</object-name>
  <object-name>Undesignate</object-name>
  <object-name>CounterMeasures</object-name>
  <object-name>Cage</object-name>
  <object-name>SpeedBrakes</object-name>
  <property>/controls/engines/engine/display-throttle</property>
	<interpolation>
		<entry><ind>-0.2</ind><dep>-0.01</dep></entry>
		<entry><ind>-0.02</ind><dep>-0.01</dep></entry>
        <entry><ind>0</ind><dep>0</dep></entry>
		<entry><ind>1</ind><dep>0</dep></entry>
	</interpolation>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

  <animation>
  <type>translate</type>
  <object-name>CounterMeasures</object-name>
  <property>/f-20/CM/button-depressed</property>
  <factor>-0.001</factor>
  <axis>
   <x>0.0</x>
   <y>1.0</y>
   <z>0.0</z>
  </axis>
 </animation>

  <animation>
  <type>translate</type>
  <object-name>SpeedBrakes</object-name>
  <property>/f-20/speedbrakes-switch</property>
  <factor>-0.001</factor>
  <axis>
   <x>1.0</x>
   <y>0.0</y>
   <z>0.0</z>
  </axis>
 </animation>

